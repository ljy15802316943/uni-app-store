.isActive {
  -webkit-animation: myfirst 0.8s ease forwards;
          animation: myfirst 0.8s ease forwards;
}
@-webkit-keyframes myfirst {
0% {
    bottom: 1rem;
    opacity: 0;
    visibility: visible;
}
50% {
    bottom: 2rem;
    opacity: 1;
}
60% {
    bottom: 2.5rem;
}
70% {
    bottom: 2rem;
}
80% {
    bottom: 2.5rem;
}
100% {
    bottom: 2rem;
}
}
@keyframes myfirst {
0% {
    bottom: 1rem;
    opacity: 0;
    visibility: visible;
}
50% {
    bottom: 2rem;
    opacity: 1;
}
60% {
    bottom: 2.5rem;
}
70% {
    bottom: 2rem;
}
80% {
    bottom: 2.5rem;
}
100% {
    bottom: 2rem;
}
}
.isActive2 {
  -webkit-animation: myfirst2 0.3s ease forwards;
          animation: myfirst2 0.3s ease forwards;
}
@-webkit-keyframes myfirst2 {
0% {
    bottom: 2rem;
}
100% {
    bottom: 1rem;
    visibility: hidden;
    opacity: 0;
}
}
@keyframes myfirst2 {
0% {
    bottom: 2rem;
}
100% {
    bottom: 1rem;
    visibility: hidden;
    opacity: 0;
}
}

